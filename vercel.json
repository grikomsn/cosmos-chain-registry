{
  "github": {
    "autoJobCancelation": true,
    "silent": true
  },
  "redirects": [
    {
      "source": "/",
      "destination": "https://github.com/grikomsn/cosmos-chain-registry",
      "permanent": false
    }
  ],
  "rewrites": [
    {
      "source": "/:name(\\w+)",
      "destination": "https://github.com/grikomsn/cosmos-chain-registry/:name/chain.json"
    },
    {
      "source": "/:name(\\w+)/chain",
      "destination": "https://github.com/grikomsn/cosmos-chain-registry/:name/chain.json"
    },
    {
      "source": "/:name(\\w+)/assetlist",
      "destination": "https://github.com/grikomsn/cosmos-chain-registry/:name/assetlist.json"
    }
  ]
}
