{
  "github": {
    "autoJobCancelation": true,
    "silent": true
  },
  "redirects": [
    {
      "source": "/",
      "destination": "https://github.com/grikomsn/cosmos-chain-registry",
      "permanent": false
    }
  ],
  "rewrites": [
    {
      "source": "/(.github|.gitignore|_IBC|LICENSE|README.md|vercel.json)",
      "destination": "/404"
    },
    {
      "source": "/:name",
      "destination": "https://static.registry.cosmos.griko.dev/:name/chain.json"
    },
    {
      "source": "/:name/chain",
      "destination": "https://static.registry.cosmos.griko.dev/:name/chain.json"
    },
    {
      "source": "/:name/assetlist",
      "destination": "https://static.registry.cosmos.griko.dev/:name/assetlist.json"
    },
    {
      "source": "/ibc/:name",
      "destination": "https://static.registry.cosmos.griko.dev/_IBC/:name.json"
    },
    {
      "source": "/:path*",
      "destination": "https://static.registry.cosmos.griko.dev/:path*"
    }
  ]
}
